{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "type": "module",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "migration:up": "yarn prisma migrate dev --schema prisma/schema.prisma",
    "migration:up:production": "yarn prisma migrate deploy --schema prisma/schema.prisma",
    "docker:up": "docker-compose -f docker-compose.yaml up -d",
    "start:dev": "nodemon",
    "start": "node dist/index.js",
    "build": "yarn clean && tsc && yarn copy-theme",
    "build:watch": "tsc -w",
    "db:clean": "prisma migrate reset --force && prisma migrate deploy",
    "seed": "node dist/prisma/seeds/run",
    "clean": "rimraf dist",
    "copy-theme": "copyfiles -u 1 src/themes/**/theme.bundle.js src/themes/**/*.css dist/"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/passwords": "^4.0.0",
    "@adminjs/prisma": "^4.0.0",
    "@adminjs/themes": "^1.0.0",
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.11.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@types/express": "^4.17.21",
    "adminjs": "^7.0.9",
    "ajv-formats": "^2.1.1",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "i18next": "^22.4.13",
    "knex": "^2.4.2",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.7",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
